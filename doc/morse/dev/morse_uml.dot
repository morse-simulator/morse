digraph G {
	fontname = "Bitstream Vera Sans"
	fontsize = 8
	bgcolor="transparent"
					    
	node [
		fontname = "Bitstream Vera Sans"
		fontsize = 8
		shape = "record"
		fillcolor="white"
		style="filled"
	]

	edge [
		fontname = "Bitstream Vera Sans"
		fontsize = 8
		color="black"
	]

	MorseAbstractObject [
		label = "{MorseAbstractObject |+on_completion : fonction\l+ local_data : OrderedDict\l+ del_function : array\l|+ name()\l+ interrupt()\l+ completed()\l+ register_service()\l }"
	]

	MorseOverlay [
